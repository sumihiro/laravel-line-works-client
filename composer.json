{
    "name": "sumihiro/laravel-line-works-client",
    "description": "LINE WORKS API client for Laravel",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Sumihiro\\LineWorksClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sumihiro\\LineWorksClient\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.9",
        "firebase/php-jwt": "^6.11",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/contracts": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sumihiro\\LineWorksClient\\LineWorksServiceProvider"
            ],
            "aliases": {
                "LineWorks": "Sumihiro\\LineWorksClient\\Facades\\LineWorks"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
